            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://fp7-novi.eu/unit.owl#derivedFrom"/>
                <owl:onClass rdf:resource="http://fp7-novi.eu/unit.owl#InformationSize"/>
