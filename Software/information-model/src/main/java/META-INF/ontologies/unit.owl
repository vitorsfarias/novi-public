        <rdfs:subPropertyOf rdf:resource="http://fp7-novi.eu/unit.owl#regexp"/>
    </owl:DatatypeProperty>
    


    <!-- http://fp7-novi.eu/unit.owl#hasValue -->

    <owl:DatatypeProperty rdf:about="http://fp7-novi.eu/unit.owl#hasValue"/>
    


